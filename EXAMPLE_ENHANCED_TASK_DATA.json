    },
    "task_card_body": {
      "elements": [
        "description (truncated)",
        "ticktick_tags (as chips)",
        "time_estimate (with clock icon)",
        "focus_time (with timer icon)"
      ]
    },
    "task_card_footer": {
      "elements": [
        "due_date (formatted, with urgency indicator)",
        "eisenhower_quadrant (as badge)",
        "parent_task_id (show 'Subtask' badge if present)"
      ]
    },
    "task_detail_view": {
      "sections": [
        "All basic info",
        "Scheduling (due_date, start_date, all_day)",
        "Reminders (reminder_time, repeat_flag)",
        "Organization (parent_task, column)",
        "Time tracking (estimate vs actual)",
        "AI Analysis (scores, quadrant, reasoning)",
        "Sync status"
      ]
    }
  },

  "api_endpoints_to_use": {
    "sync_from_ticktick": "POST /api/tasks/sync?user_id=1",
    "get_unsorted_tasks": "GET /api/tasks/unsorted?user_id=1",
    "get_tasks_by_quadrant": "GET /api/tasks?user_id=1&quadrant=Q1",
    "get_projects": "GET /api/projects?user_id=1",
